<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/navigation_my_learning">

    <fragment
        android:id="@+id/navigation_my_learning"
        android:name="com.example.helpme.MyLearningFragment"
        android:label="My Learning"
        tools:layout="@layout/fragment_my_learning" >
        <argument
            android:name="nickname"
            app:argType="string" />
        <argument
            android:name="email"
            app:argType="string" />
        <argument
            android:name="profile_image"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/navigation_explore"
        android:name="com.example.helpme.ExploreFragment"
        android:label="둘러보기"
        tools:layout="@layout/fragment_explore" />
</navigation>
